public class Rectangle extends Shape
{
  private double height;
    private double width;
    public Rectangle()
    {
      this(1.0, 2.0);
    }
    public Rectangle(double h, double w)
    {
     height = h;
     width = w;
     super.setType("Rectangle ");
    }
    public double getArea()
    {
     return height * width; 
    }
    
    public void print()
    {
     System.out.print(getArea()); 
    }
    
}